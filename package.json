{
  "name": "vite-reactts-eslint-prettier",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --ext .jsx,.js,.ts,.tsx --ignore-path .gitignore .",
    "ts": "tsc",
    "test": "jest",
    "prettier": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js, ts, tsx}": "npm run lint",
    "*.{js, ts, tsx, json}": "prettier --cache --write"
  },
  "dependencies": {
    "@mui/material": "^7.0.2",
    "@mui/styled-engine-sc": "^7.0.2",
    "effector": "^23.3.0",
    "effector-react": "^23.3.0",
    "i18next": "^25.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.5.1",
    "react-svg": "^16.3.0",
    "styled-components": "^6.1.17"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "@babel/preset-env": "^7.27.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.5.1",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vite-plugin-svgr": "4.3.0",
    "lint-staged": "^15.5.1"
  },
  "resolutions": {
    "styled-components": "^5",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "license": "MIT"
}
